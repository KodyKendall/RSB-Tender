<div id="<%= dom_id project %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Rsb number:</strong>
    <%= project.rsb_number %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Tender:</strong>
    <%= project.tender_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Project status:</strong>
    <%= project.project_status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Project start date:</strong>
    <%= project.project_start_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Project end date:</strong>
    <%= project.project_end_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Budget total:</strong>
    <%= project.budget_total %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Actual spend:</strong>
    <%= project.actual_spend %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Created by:</strong>
    <%= project.created_by_id %>
  </div>
</div>
