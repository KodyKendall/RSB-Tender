<div id="<%= dom_id variation_order %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Vo number:</strong>
    <%= variation_order.vo_number %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Project:</strong>
    <%= variation_order.project_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Vo status:</strong>
    <%= variation_order.vo_status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Vo amount:</strong>
    <%= variation_order.vo_amount %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= variation_order.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Created by:</strong>
    <%= variation_order.created_by_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Approved by:</strong>
    <%= variation_order.approved_by_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Approver notes:</strong>
    <%= variation_order.approver_notes %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Approved at:</strong>
    <%= variation_order.approved_at %>
  </div>
</div>
